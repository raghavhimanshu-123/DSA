class Solution {
  public:
    bool isPalindrome(string& s) {
        int n = s.length();
        
        int st=0 , end = n-1;
        while(st<=end){
            if(s[st] != s[end]){
                return false;
            }
            st++; end--;
        }
        return true;
      
        
    }
};
